import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner inn=new Scanner(System.in);
        float a=inn.nextFloat();
        System.out.printf("%.6f\n%.4f\n%.2f\n%d",a,a,a,(int)Math.round(a));
    }
}
