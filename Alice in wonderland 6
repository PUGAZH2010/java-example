import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner inn=new Scanner(System.in);
        int a=inn.nextInt();
        int b=a/10;
        int c=a %10;
        int d=b+c;
        System.out.println("Bird said:"+a);
        System.out.println("Alice must go in path-"+d);
 
    }
}
